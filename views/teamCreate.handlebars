<div class="card col-6">
  <div class="card-header">
    Create Team
  </div>
  <div class="card-block">
    <form class="form-group">	
			<div class="form-group row">
				<label for="example-text-input" class="col-3 col-form-label">Entry Name:</label>
				<div class="col-3"></div>
				<div class="col-6">
					<input class="form-control" type="text" id="entryName" style="float: right">
				</div>
			</div>
			<div class="form-group row">
				<label class="col-3 col-form-label" for="">1 - 5</label>
				<div class="col-3"></div>
				<div class="col-6">
						<select class="golfers custom-select" id="golfer1" style="float: right">
							<option value="Select Golfer">Select Golfer</option>
							{{#each golfers}}
							<option value="{{name}}" data-rank="{{currentRank}}">{{currentRank}}. {{name}}</option>
							{{/each}}
						</select>
				</div>
			</div>
			<div class="form-group row">
				<label class="col-3 col-form-label" for="">6 - 10</label>
				<div class="col-3"></div>
				<div class="col-6">
						<select class="golfers custom-select" id="golfer2" style="float: right">
							<option value="Select Golfer">Select Golfer</option>
							{{#each golfers}}
							<option value="{{name}}" data-rank="{{currentRank}}">{{currentRank}}. {{name}}</option>
							{{/each}}
						</select>
				</div>
			</div>
			<div class="form-group row">
				<label class="col-3 col-form-label" for="">11 - 20</label>
				<div class="col-3"></div>
				<div class="col-6">
						<select class="golfers custom-select" id="golfer3" style="float: right">
							<option value="Select Golfer">Select Golfer</option>
							{{#each golfers}}
							<option value="{{name}}" data-rank="{{currentRank}}">{{currentRank}}. {{name}}</option>
							{{/each}}
						</select>
				</div>
			</div>
						<div class="form-group row">
				<label class="col-3 col-form-label" for="">11 - 20</label>
				<div class="col-3"></div>
				<div class="col-6">
						<select class="golfers custom-select" id="golfer4" style="float: right">
							<option value="Select Golfer">Select Golfer</option>
							{{#each golfers}}
							<option value="{{name}}" data-rank="{{currentRank}}">{{currentRank}}. {{name}}</option>
							{{/each}}
						</select>
				</div>
			</div>
						<div class="form-group row">
				<label class="col-3 col-form-label" for="">21 - 50</label>
				<div class="col-3"></div>
				<div class="col-6">
						<select class="golfers custom-select" id="golfer5" style="float: right">
							<option value="Select Golfer">Select Golfer</option>
							{{#each golfers}}
							<option value="{{name}}" data-rank="{{currentRank}}">{{currentRank}}. {{name}}</option>
							{{/each}}
						</select>
				</div>
			</div>
			<div class="form-group row">
				<label class="col-3 col-form-label" for="">51 +</label>
				<div class="col-3"></div>
				<div class="col-6">
						<select class="golfers custom-select" id="golfer6" style="float: right">
							<option value="Select Golfer">Select Golfer</option>
							{{#each golfers}}
							<option value="{{name}}" data-rank="{{currentRank}}">{{currentRank}}. {{name}}</option>
							{{/each}}
						</select>
				</div>
			</div>
			<button type="submit" class="btn btn-primary" id="teamSubmit" style="float: right; padding: 10px;">Submit Team</button>
		</form>
  </div>
</div>

	
